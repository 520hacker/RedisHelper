<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Colin Chang</Authors>
    <Copyright>https://colinchang.net/</Copyright>
    <PackageProjectUrl>https://github.com/colin-chang/RedisHelper</PackageProjectUrl>
    <RepositoryUrl>https://github.com/colin-chang/RedisHelper</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>redis redis.net redis.driver redis.helper redis.net.core</PackageTags>
    <Description>a common redis helper based on StackExchange.Redis.</Description>
    <AssemblyVersion>1.1.4.0</AssemblyVersion>
    <FileVersion>1.1.4.0</FileVersion>
    <PackageReleaseNotes>update lockexecute to be generic</PackageReleaseNotes>
    <Version>1.2.0</Version>
    <PackageIconUrl>https://raw.githubusercontent.com/colin-chang/RedisHelper/master/redis.ico</PackageIconUrl>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageVersion>1.1.4</PackageVersion>
    <Title>RedisHelper</Title>
    <LangVersion>8</LangVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>C:\Users\zhang\Downloads\RedisHelper\ColinChang.RedisHelper\ColinChang.RedisHelper.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DocumentationFile>bin\Release\ColinChang.RedisHelper.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="StackExchange.Redis" Version="2.0.601" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
</Project>
